{
    "source": {
      "include": ["./", "README.md"],
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "node_modules"
    },
    "sourceType": "module",
    "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc", "closure"]
    },
    "plugins": ["plugins/markdown"],
    "recurse": 10,
    "opts": {
      "template": "node_modules/docdash",
      "destination": "./docs/",
      "recurse": true
    },
    "markdown": {
      "idInHeadings": true
    },
    "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
    },
      "docdash:": {
        "sort": true,
        "search": true,
        "collapse:": false
      }
  }